WITH
1 AS (
SELECT *
    FROM
        {{ ref('dim_listings_cleansed') }}
),
h AS (
SELECT *
    FROM {{ ref('dim_hosts_cleansed') }}
)
SELECT
    1.listing_id,
    1.listing_name,
    1.room_type,
    1.minimum_nights,
    1.price,
    1.host_id,
    h.host_name,
    h.is_superhost as host_is_superhost,
    1.created_at,
    GREATEST(1.updated_at, h.updated_at) as updated_at
FROM 1
LEFT JOIN h ON (h.host_id = 1.host_id)